{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": [
        "error",
        {
            "singleQuote": true,
            "trailingComma": "all",
            "printWidth": 100
        }
        ]
    },
    "globals": {
        "browser": true
    }
}
